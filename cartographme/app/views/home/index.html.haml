/ Home page
%table 
  -if @current_user
    = button_to 'My Profile', @current_user, :method => :get, :id => 'profile_button', :class => "button" 
    = button_to "Search Users", users_path, :method => :get, :id => 'search_button', :class => "button" 
    = button_to 'Logout', logout_path , :method => :delete , :id => 'logout', :class => "button"
    #div{:class => "card"}
      #map{:style => "width:50; height:550px;"}
  -else
    %h2
      = button_to 'Sign up/Login', login_path , :method => :get, :id => 'signuplogin', :class => "button" 

